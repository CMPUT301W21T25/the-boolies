<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/successRate"
                android:layout_width="414dp"
                android:layout_height="40dp"
                android:text="TextView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/stDev" />

            <TextView
                android:id="@+id/stDev"
                android:layout_width="414dp"
                android:layout_height="40dp"
                android:text="TextView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/mean" />

            <TextView
                android:id="@+id/mean"
                android:layout_width="414dp"
                android:layout_height="40dp"
                android:text="TextView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/median" />

            <TextView
                android:id="@+id/median"
                android:layout_width="414dp"
                android:layout_height="40dp"
                android:text="TextView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/quartiles" />

            <TextView
                android:id="@+id/quartiles"
                android:layout_width="414dp"
                android:layout_height="40dp"
                android:text="TextView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/conductedTrials" />

            <TextView
                android:id="@+id/conductedTrials"
                android:layout_width="414dp"
                android:layout_height="40dp"
                android:text="TextView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/minimumTrials" />

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/viewExperimentDataInfo"
                android:layout_width="0dp"
                android:layout_height="180dp"
                android:background="@color/custom_Yellow_dark"
                android:minHeight="?attr/actionBarSize"
                android:theme="?attr/actionBarTheme"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:title="Testing Title"
                app:titleMargin="5dp"
                tools:subtitle="Testing Date" />

            <TextView
                android:id="@+id/viewExperimentDataDescription"
                android:layout_width="414dp"
                android:layout_height="144dp"
                android:text="TextView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/viewExperimentDataInfo" />

            <TextView
                android:id="@+id/minimumTrials"
                android:layout_width="414dp"
                android:layout_height="40dp"
                android:text="TextView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/viewExperimentDataDescription" />

            <Button
                android:id="@+id/viewGraphsButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="60dp"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="60dp"
                android:text="View Graphs"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/successRate"
                tools:backgroundTint="@color/custom_Blue_dark" />

            <Button
                android:id="@+id/viewMapsButton"
                android:layout_width="140dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="60dp"
                android:layout_marginEnd="60dp"
                android:layout_marginRight="60dp"
                android:text="View Map"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/viewGraphsButton"
                app:layout_constraintTop_toBottomOf="@+id/successRate"
                tools:backgroundTint="@color/custom_Blue_dark" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>